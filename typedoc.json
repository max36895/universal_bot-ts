{
    "entryPoints": ["src", "cli"],
    "entryPointStrategy": "expand",
    "readme": "README.md",
    "name": "umbot",
    "out": "./docs/",
    "tsconfig": "tsconfigForDoc.json",
    "excludePrivate": true,
    "excludeExternals": true,
    "excludeInternal": true,
    "excludeReferences": true,
    "theme": "default",
    "includeVersion": true,
    "categorizeByGroup": true,
    "searchInComments": true,
    "sort": ["kind", "instance-first", "alphabetical"],
    "validation": {
        "invalidLink": true,
        "notDocumented": false
    },
    "navigationLinks": {
        "GitHub": "https://github.com/max36895/universal_bot-ts",
        "npm": "https://www.npmjs.com/package/umbot"
    },
    "visibilityFilters": {
        "protected": false,
        "private": false,
        "inherited": true,
        "external": false
    }
}
